select * from country LEFT JOIN city on country.country_id = city.country_id;

select p.payment_id, c.first_name, c.last_name from customer c RIGHT JOIN payment p on p.customer_id = c.customer_id;

select r.rental_id, c.first_name, c.last_name from customer c FULL JOIN rental r on c.customer_id = r.customer_id;
